<div class="mdl-grid load-animations">
    {{#each this}}
        <div class="mdl-cell">
            <div class="mdl-card mdl-shadow--2dp tile">
                <div class="mdl-card__title mdl-card--expand">
                    {{#each mutations}}
                        <h2 class="mdl-card__title-text">{{root}}</h2>
                    {{/each}}
                </div>
            </div>
        </div>
    {{/each}}
</div>